{
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "strict": [2, "global"],
    "comma-dangle": [2, "never"]
  },
  "settings": {
    "import/resolver": "node"
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ]
}